<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="157.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hemsteam.hems.controllers.LoginController">
   <center>
      <GridPane BorderPane.alignment="CENTER">
         <children>
            <TextField id="userName" fx:id="userName" maxHeight="1.7976931348623157E308" promptText="请输入用户名">
               <GridPane.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
               </GridPane.margin>
            </TextField>
            <PasswordField id="passWord" fx:id="passWord" maxHeight="1.7976931348623157E308" promptText="请输入密码" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
               </GridPane.margin>
            </PasswordField>
            <GridPane prefHeight="57.0" prefWidth="258.0" GridPane.rowIndex="3">
               <children>
                  <Button id="clear" cancelButton="true" maxHeight="1.7976931348623157E308"
                          maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClearButtonClick"
                          text="清除" GridPane.columnIndex="1">
                     <GridPane.margin>
                        <Insets bottom="10.0" left="2.5" right="10.0"/>
                     </GridPane.margin>
                  </Button>
                  <Button fx:id="resigster" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                          mnemonicParsing="false" text="注册">
                     <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="2.5"/>
                     </GridPane.margin>
                  </Button>
               </children>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               </rowConstraints>
            </GridPane>
            <Button id="login" defaultButton="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                    mnemonicParsing="false" onAction="#onLoginButtonClick"
                    styleClass="btn-basic,green-theme,btn-radius-normal" text="登录" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
               </GridPane.margin>
            </Button>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
         </rowConstraints>
         <padding>
            <Insets top="5.0"/>
         </padding>
      </GridPane>
   </center>
</BorderPane>
