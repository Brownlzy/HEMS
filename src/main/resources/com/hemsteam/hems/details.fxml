<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root prefHeight="483.0" prefWidth="679.0" type="javafx.scene.layout.BorderPane" AnchorPane.bottomAnchor="0.0"
         AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
         xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="com.hemsteam.hems.controllers.DetailsController">
    <top>
        <AnchorPane prefHeight="38.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Label alignment="CENTER" layoutX="-1.0" layoutY="-3.0" prefHeight="41.0" prefWidth="331.0"
                       text="明细"/>
            </children>
        </AnchorPane>
    </top>
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="381.0" minWidth="10.0" prefWidth="381.0"/>
                        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="0.0"
                                           prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <MenuButton mnemonicParsing="false" text="排序方式">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Action 1"/>
                                <MenuItem mnemonicParsing="false" text="Action 2"/>
                            </items>
                        </MenuButton>
                        <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="75.0" GridPane.columnIndex="2">
                            <children>
                                <Button mnemonicParsing="false" onAction="#onAddClick" text="增加"/>
                                <Button mnemonicParsing="false" onAction="#onEnableEdit" text="返回">
                                    <HBox.margin>
                                        <Insets left="10.0"/>
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1"/>
                    </children>
                </GridPane>
                <TableView fx:id="detailsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                           GridPane.rowIndex="1">
                    <columns>
                        <TableColumn fx:id="timeColumn" prefWidth="75.0" text="时间"/>
                        <TableColumn fx:id="moneyColumn" prefWidth="75.0" text="金额"/>
                        <TableColumn fx:id="positionColumn" prefWidth="75.0" text="地点"/>
                        <TableColumn fx:id="typeColumn" prefWidth="107.0" text="类型"/>
                        <TableColumn fx:id="tipColumn" minWidth="-1.0" prefWidth="181.0" text="备注"/>
                        <TableColumn prefWidth="150.0" text="操作">
                            <columns>
                                <TableColumn prefWidth="70.0" text="编辑"/>
                                <TableColumn text="删除"/>
                            </columns>
                        </TableColumn>
                    </columns>
                </TableView>
            </children>
        </GridPane>
    </center>
</fx:root>
